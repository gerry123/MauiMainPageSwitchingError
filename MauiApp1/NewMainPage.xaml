<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.NewMainPage"
             BackgroundColor="LightPink"
             Title="NewMainPage">
  <VerticalStackLayout
             Spacing="25"
             Padding="30,0"
             VerticalOptions="Center">
    <Button
                x:Name="SetOrgMainPage"
                Text="Set original MainPage"
                Clicked="OnSetOrgMainPageClicked"
                HorizontalOptions="Center" />
    <Button
                x:Name="GetNavStackBtn"
                Text="Get nav stack"
                Clicked="OnGetNavStackBtnClicked"
                HorizontalOptions="Center" />
    <Label x:Name="laNavStack" Margin="0,20" Text="**************" HorizontalOptions="Center" MaxLines="10" />
  </VerticalStackLayout>
</ContentPage>